def prime_number(w):
    n= True
    y=0
    if w >=2:
        for i in range(1,w):
            x = w % i
            if x==0:
               y=y+1
               if y==1:
                   n=n
               elif y>=2:
                    n=not n

    else:
        n=not n
    return n
b=prime_number(77)
print(b)
